<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    style="@style/dialog_backgroud">
    <ImageView
        android:id="@+id/iv_emo"
        style="@style/img_popup"/>
    <TextView
        android:id="@+id/tv1"
        style="@style/msg_success_scan_recto_line1"/>
    <TextView
        android:id="@+id/tv2"
        style="@style/msg_success_scan_recto_line2"/>
    <LinearLayout
        android:orientation="horizontal"
        android:id="@+id/ll"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/iv_annuler"
            style="@style/btn_annuler_popup"/>
        <TextView
            android:id="@+id/iv_continuer"
            style="@style/btn_continuer_popup"/>
    </LinearLayout>
</LinearLayout>
